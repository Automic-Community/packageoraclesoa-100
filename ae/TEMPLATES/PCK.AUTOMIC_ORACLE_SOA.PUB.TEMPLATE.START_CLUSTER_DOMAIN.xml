<SCRIPT mode="1" state="1">
<MSCRI><![CDATA[! Common variables
:SET &CONFIGURE_OBJECT# = "PCK.AUTOMIC_ORACLE_SOA.PUB.VARA.DOMAIN_CONFIGURATIONS"
:SET &ENVIRONMENT# = "&UC4RB_WEBLOGIC_ENVIRONMENT#"
:SET &FILL_OUT_MESSAGE# = "&CONFIGURE_OBJECT# object"

:PUBLISH &ENVIRONMENT#,,"WORKFLOW"
:PUBLISH &CONFIGURE_OBJECT#,,"WORKFLOW"
:PUBLISH &FILL_OUT_MESSAGE#,,"WORKFLOW"

! AdminServer Name
:SET &HND# = PREP_PROCESS_VAR_XML(&CONFIGURE_OBJECT#,&UC4RB_WEBLOGIC_ENVIRONMENT#, '//domains/domain[@name="&UC4RB_WEBLOGIC_DOMAIN#"]/machines/machine/admin-server/@name')
:PROCESS &HND#
: SET &VALUE# = GET_PROCESS_LINE(&HND#,1)
: IF &VALUE# <> ""
:   SET &UC4RB_WEBLOGIC_ADMIN_NAME# = "&VALUE#"
: ENDIF
:ENDPROCESS
:CLOSE_PROCESS &HND#
:PUBLISH &UC4RB_WEBLOGIC_ADMIN_NAME#,,"WORKFLOW"

!Host
:SET &HND# = PREP_PROCESS_VAR_XML(&CONFIGURE_OBJECT#, &UC4RB_WEBLOGIC_ENVIRONMENT#, '//domains/domain[@name="&UC4RB_WEBLOGIC_DOMAIN#"]/machines/machine/admin-server/@host')
:PROCESS &HND#
: SET &VALUE# = GET_PROCESS_LINE(&HND#,1)
: IF &VALUE# <> ""
:   SET &UC4RB_WEBLOGIC_HOST# = "&VALUE#"
: ENDIF
:ENDPROCESS
:CLOSE_PROCESS &HND#
:PUBLISH &UC4RB_WEBLOGIC_HOST#,,"WORKFLOW"

!Port
:SET &HND# = PREP_PROCESS_VAR_XML(&CONFIGURE_OBJECT#, &UC4RB_WEBLOGIC_ENVIRONMENT#, '//domains/domain[@name="&UC4RB_WEBLOGIC_DOMAIN#"]/machines/machine/admin-server/@port')
:PROCESS &HND#
: SET &VALUE# = GET_PROCESS_LINE(&HND#,1)
: IF &VALUE# <> ""
:   SET &UC4RB_WEBLOGIC_PORT# = "&VALUE#"
: ENDIF
:ENDPROCESS
:CLOSE_PROCESS &HND#

!Check if PORT equal ""
:IF &UC4RB_WEBLOGIC_PORT# = ""
:  STOP MSG, 50, "domain[@name='&UC4RB_WEBLOGIC_DOMAIN#']/machines/machine/admin-server/@port is missing. Please fillout it in &CONFIGURE_OBJECT# object"
:ENDIF
:PUBLISH &UC4RB_WEBLOGIC_PORT#,,"WORKFLOW"

! Global settings
!Timeout
:SET &HND# = PREP_PROCESS_VAR_XML(&CONFIGURE_OBJECT#, &UC4RB_WEBLOGIC_ENVIRONMENT#, '//domains/domain[@name="&UC4RB_WEBLOGIC_DOMAIN#"]/global-settings/global-setting[@name="timeout"]/@value')
:PROCESS &HND#
